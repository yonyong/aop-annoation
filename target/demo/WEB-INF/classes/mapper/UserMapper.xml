<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.dao.UserDao">
	<select id="queryUser" resultType="com.cn.entity.User">
		SELECT
			id,
			username,
			PASSWORD,
			create_time,
			last_edit_time
		FROM
			t_user
	</select>
</mapper>